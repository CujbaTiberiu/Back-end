package it.epicode.be;

public class Helloworld {

public static void main (String[] args)
		{
	int num1 = 10;
	int num2 = 5;
	int multiplyResult = num1 * num2;
	System.out.println(multiplyResult);
	
	int age = 27;
	String name = "Tiberio";
	String nameAge = name + ' ' +  "is" + ' '  + age + ' ' + "years old";
	System.out.println(nameAge);
	
	String insersciInArray[] = new String[5];
	insersciInArray[2]="boh";
	System.out.println(insersciInArray);
		}

}



